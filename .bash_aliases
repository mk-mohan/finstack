alias my-pistack_up="docker-compose -f ~/my-pistack/docker-compose.yml up -d"
alias my-pistack_down="docker-compose -f ~/my-pistack/docker-compose.yml down"
alias my-pistack_start="docker-compose -f ~/my-pistack/docker-compose.yml start"
alias my-pistack_stop="docker-compose -f ~/my-pistack/docker-compose.yml stop"
alias my-pistack_update="docker-compose -f ~/my-pistack/docker-compose.yml pull"
alias my-pistack_build="docker-compose -f ~/my-pistack/docker-compose.yml build"

